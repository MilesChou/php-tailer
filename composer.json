{
    "name": "tailer/tailer",
    "description": "Simulate tail command using PHP",
    "type": "library",
    "license": "MIT License",
    "authors": [
        {
            "name": "MilesChou",
            "email": "jangconan@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5 | >=7.0",
        "watcher/watcher": "^0.5.0"
    },
    "require-dev": {
        "monolog/monolog": "^1.22",
        "squizlabs/php_codesniffer": "^2.7",
        "symfony/console": "^3.2",
        "symfony/process": "^3.2"
    },
    "suggest": {
        "symfony/console": "Watch your log using console now",
        "symfony/process": "Use Process if you will run in Linux system"
    },
    "autoload": {
        "psr-4": {
            "Tailer\\": "src"
        }
    },
    "scripts": {
        "test": [
            "php vendor/bin/phpcs"
        ]
    }
}
